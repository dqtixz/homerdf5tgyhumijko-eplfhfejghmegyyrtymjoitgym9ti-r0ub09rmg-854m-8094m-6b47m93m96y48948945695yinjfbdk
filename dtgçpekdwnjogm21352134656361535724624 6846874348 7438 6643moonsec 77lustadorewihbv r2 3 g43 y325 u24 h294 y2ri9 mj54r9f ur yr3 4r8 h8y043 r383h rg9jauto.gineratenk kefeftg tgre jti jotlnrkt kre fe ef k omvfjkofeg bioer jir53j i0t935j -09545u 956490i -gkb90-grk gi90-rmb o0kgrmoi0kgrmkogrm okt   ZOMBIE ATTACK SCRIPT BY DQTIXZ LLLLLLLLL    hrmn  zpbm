local PLTab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://5009915812",
	PremiumOnly = false
})

local Section = PLTab:AddSection({
	Name = "Client"
})

PLTab:AddLabel("Speed, Jump Ect...")

